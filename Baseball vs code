#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int na(char arr[][10], int n) {
    int top = -1;
    int stack[n];

    for (int i = 0; i < n; i++) {
        if (strcmp(arr[i], "+") == 0) {
            int val1 = stack[top];
            int val2 = stack[top - 1];
            stack[++top] = val1 + val2;
        } else if (strcmp(arr[i], "D") == 0) {
            int val = stack[top]; // â† store the current top value first
            stack[++top] = 2 * val;
        } else if (strcmp(arr[i], "C") == 0) {
            top--;
        } else {
            stack[++top] = atoi(arr[i]);
        }
    }

    int sum = 0;
    while (top >= 0) {
        sum += stack[top--];
    }

    return sum;
}

int main() {
    int n;
    printf("Enter size: ");
    scanf("%d", &n);
    
    char arr[100][10];
    printf("Enter strings:\n");
    for (int i = 0; i < n; i++)
        scanf("%s", arr[i]);

    int result = na(arr, n);
    printf("Output: %d\n", result);
    return 0;
}

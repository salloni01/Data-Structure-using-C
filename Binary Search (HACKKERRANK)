#include<stdio.h>
#include<stdlib.h>
int main(){
    int n;
    printf("Enter Array Size:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter Array Elements");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }

int target;
printf("Enter Searching Element:");
scanf("%d",&target);
int start=0,end=n-1;
while(start<=end){
    int mid=(start+end)/2;
    if(arr[mid]==target){
        break;
    }
    else if(target<arr[mid])
    end=mid-1;
    else
    start=mid+1;
}
if(start<=end)
printf("Found !!!!");
else 
printf(" Not Found !!!!");
return 0;
}

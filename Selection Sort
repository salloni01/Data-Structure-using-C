#include<stdio.h>
#include<stdlib.h>
int main(){
    int n;
    printf("Enter Array Size:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter Array Elements");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    // selection sort
    for(int i=0;i<n-1;i++){
int minIndex=i;
for(int j=i+1;j<n;j++){
    if(arr[j]<arr[minIndex])
        minIndex=j;
    
}
if(i != minIndex){
    int temp=arr[i];
    arr[i]=arr[minIndex];
    arr[minIndex]=temp;
}
    }
printf("After Sorting Array Elements Are : \n");
for(int i=0;i<n;i++){
printf("%d ",arr[i]);  
 }

return 0;
}
